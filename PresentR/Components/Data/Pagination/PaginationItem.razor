@namespace PresentR.Components
@inherits PresentrComponent

<li @attributes="AdditionalAttributes" class="@ClassString">
    <a class="page-link" href="#" @onclick:preventDefault @onclick="OnClickItem">
        @if (ChildContent != null)
        {
            @ChildContent
        }
        else
        {
            @Index
        }
    </a>
</li>
